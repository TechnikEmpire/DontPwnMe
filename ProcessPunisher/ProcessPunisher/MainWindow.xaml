<Window x:Class="ProcessPunisher.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:local="clr-namespace:ProcessPunisher"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        Title="I'M HERE! GET SOME!"
        Width="525"
        Height="481"
        WindowStartupLocation="CenterScreen"
        mc:Ignorable="d">
    <Grid Margin="0,0,0,-5" Background="Black">
        <!--  Image from http://pre03.deviantart.net/b61e/th/pre/f/2014/296/e/f/punisher_by_ururuty-d83v3fn.jpg  -->
        <Image x:Name="image"
               Width="300"
               Height="299"
               HorizontalAlignment="Center"
               VerticalAlignment="Top"
               Source="/ProcessPunisher;component/Inc/punisher_by_ururuty-d83v3fn.jpg" />
        <TextBlock x:Name="victimsList"
                   Width="375"
                   Height="78"
                   Margin="0,329,0,0"
                   HorizontalAlignment="Center"
                   VerticalAlignment="Top"
                   Foreground="White"
                   Text="Scum Killed"
                   TextAlignment="Center"
                   TextWrapping="Wrap" />

    </Grid>
</Window>
